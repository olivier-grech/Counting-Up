for (var i = 0; i &lt; grid_size; i++) {
    for (var j = 0; j &lt; grid_size; j++) {
        
        buildings_guessed[i, j] = 1;
    }
}

//Create indicators showing the number of visible buildings
for (var i = 0; i &lt; grid_size; i++) { 
    temp = 0;
    temp_max = 0;
    for (var j = 0; j &lt; grid_size; j++) {
        if (buildings[i, j] &gt; temp_max) {
            temp_max = buildings[i, j];
            temp++;
        }
    }
    indicator_up[i] = temp;
}

